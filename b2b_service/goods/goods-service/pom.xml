        <dependency>
            <groupId>com.bat</groupId>
            <artifactId>dubboapi-warehouse</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.bat</groupId>
            <artifactId>goods-api</artifactId>
