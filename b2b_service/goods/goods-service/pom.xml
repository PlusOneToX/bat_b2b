    <artifactId>goods-service</artifactId>
    <version>1.0</version>

    <repositories>
        <repository>
            <id>dubboapi</id>
            <name>dubboapi-public</name>
            <url>http://127.0.0.1:8071/repository/dubboapi-public/</url>
            <releases>
