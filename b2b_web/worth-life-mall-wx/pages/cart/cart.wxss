  line-height: 24rpx;
  font-size: 22rpx;
  color: #666;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.cart-view .item .goods-do {
  position: absolute;
  right: 0rpx;
  top: 10rpx;
}

.number {
  position: absolute;
  right: 0rpx;
