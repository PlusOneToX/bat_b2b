  margin: 0 18rpx;
  background: #3c3c3c44;
  font-size: 29rpx;
  color: #fff;
  border-radius: 25rpx;
}

.cart-bottom .checkout {
  height: 50rpx;
  width: 150rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 29rpx;
  
  background: linear-gradient(270deg, #F96319 0%, #F92C19 100%);
  color: #fff;
  border-radius: 25rpx;
}

