  font-weight: 400;
  color: #333333;
  line-height: 40rpx;

}

.cart-bottom>label.tui-checkbox checkbox{
  padding-right: 10rpx;
}

.cart-bottom .checkbox.checked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 40rpx;
}



.cart-bottom .total {
  flex: 1;
  overflow: hidden;
  color: #e41f19;
  justify-content: flex-start;
  box-sizing: border-box;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  /* color: #333333; */
  line-height: 40rpx;
}

.cart-bottom .delete {
  height: 50rpx;
  width: 150rpx;
