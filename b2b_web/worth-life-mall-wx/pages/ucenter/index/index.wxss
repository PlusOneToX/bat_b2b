page {
  background-color: #FFFFFF;
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
}

.tui-header {
  width: 100%;
  font-size: 18px;
  line-height: 18px;
  font-weight: 500;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* #ifndef MP */
.tui-header-icon {
  position: fixed;
  top: 0;
  right: 15px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 32px;
  transform: translateZ(0);
  z-index: 99999;
}

/* #endif */
/* #ifdef MP */
.tui-set-box {
  flex: 1;
  display: flex;
  justify-content: flex-end;
  
}

.tui-set-box .tui-icon-box{
  width: 42rpx;
  height: 42rpx;
}

.tui-set-box .tui-icon-box image{
  width: 100%;
  height: 100%;
}

/* #endif */
.tui-icon-box {
  position: relative;
}

.tui-icon-setup {
  margin-left: 16rpx;
}

.tui-badge {
  position: absolute;
  font-size: 24rpx;
  height: 32rpx;
  min-width: 20rpx;
  padding: 0 6rpx;
  border-radius: 40rpx;
  right: 10rpx;
  top: -5rpx;
  transform: scale(0.8) translateX(60%);
  transform-origin: center center;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10;
}

.tui-badge-red {
  background: #F74D54;
  color: #fff;
}

.tui-badge-white {
  background: #fff;
  color: #F74D54;
}

.tui-badge-dot {
  position: absolute;
  height: 12rpx;
  width: 12rpx;
  border-radius: 50%;
  right: -12rpx;
  top: 0;
  background: #F74D54;
}

.tui-mybg-box {
  width: 100%;
  height: 512rpx;
  position: relative;
}

.tui-my-bg {
  width: 100%;
  height: 512rpx;
  display: block;
}

.tui-header-center {
  position: absolute;
  width: 100%;
  left: 0;
  top: 190rpx;
  padding: 0 63rpx;
  padding-top: env(safe-area-inset-top);
  box-sizing: border-box;
  display: flex;

}

.tui-avatar {
  flex-shrink: 0;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  display: inline-block;
}

.tui-info {
  margin-left: 20rpx;
 display: flex;
 flex-direction: column;
 justify-content: center;
 align-items: flex-start;
}

.tui-nickname {
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}
.tui-nickname text{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
  background: #FFFFFF;
  text-align: center;
  line-height: 40rpx;
  border-radius: 20rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #1A1919;
  color:  #FE8C00;
}

.tui-tag{
  margin-top: 16rpx;
  padding: 0 20rpx;
  height: 30rpx;
  background: linear-gradient(270deg, #B3A0A3 0%, #FDE9DD 100%);
  border-radius: 15rpx;
  font-size: 16rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #686363;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tui-img-vip {
  width: 56rpx;
  height: 24rpx;
  margin-left: 18rpx;
}

.tui-explain {
  width: 80%;
  font-size: 24rpx;
  font-weight: 400;
  color: #fff;
  opacity: 0.75;
  padding-top: 8rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.tui-btn-edit {
  flex-shrink: 0;
  padding-right: 22rpx;
}

.tui-header-btm {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  top: 337rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
}

.tui-btm-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.tui-btm-num {

  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 42rpx;
  position: relative;
}

.tui-btm-text {

  font-size: 26rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: #FEFFFF;
  line-height: 37rpx;
  padding-top: 4rpx;
}

.tui-content-box {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
  top: -72rpx;
  z-index: 10;
}

.tui-box {
  width: 100%;
  background: #fff;
  box-shadow: 0 3rpx 20rpx rgba(183, 183, 183, 0.1);
  border-radius: 10rpx;
  overflow: hidden;
}

.tui-order-box {
  height: 248rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 20rpx 2rpx rgba(0, 0, 0, 0.03);
  border-radius: 30rpx;
  box-sizing: border-box;
  padding: 30rpx 0;
}

.tui-cell-header {
  width: 100%;
  padding: 0 26rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.tui-cell-header .tui-cell-title{
  flex: 1;
  font-size: 34rpx;
  font-family: SourceHanSansCN-Normal, SourceHanSansCN;
  font-weight: 400;
  color: #606E73;
  line-height: 51rpx;
}

.tui-cell-header .right-icon{
  width: 44rpx;
  height: 44rpx;
}

.tui-cell-title {
  font-size: 30rpx;
  line-height: 30rpx;
  font-weight: 600;
  color: #333;
}

.tui-cell-sub {

  font-size: 28rpx;
  font-family: SourceHanSansCN-Normal, SourceHanSansCN;
  font-weight: 400;
  color: #999999;
  line-height: 42rpx;
  /* padding-right: 28rpx; */
}

.tui-order-list {
  width: 100%;
  padding: 0 30rpx 0;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.tui-order-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tui-order-text{

  font-size: 28rpx;
  font-family: SourceHanSansCN-Normal, SourceHanSansCN;
  font-weight: 400;
  color: #333333;
  line-height: 42rpx;
  padding-top: 4rpx;
}

.tui-tool-text {
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 42rpx;
}

.tui-order-icon {
  width: 66rpx;
  height: 66rpx;
  display: block;
}

.tui-assets-box {
  height: 178rpx;
  margin-top: 20rpx;
}

.tui-assets-list {
  height: 84rpx;
}

.tui-assets-num {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
  position: relative;
}

.tui-assets-text {
  font-size: 24rpx;
  font-weight: 400;
  color: #666;
  padding-top: 6rpx;
}

.tui-tool-box {
  margin-top: 20rpx;
  box-shadow: 0rpx 2rpx 20rpx 2rpx rgba(0, 0, 0, 0.03);
  border-radius: 21rpx;
}

.tui-flex-wrap {
  flex-wrap: wrap;
  height: auto;
  padding-bottom: 30rpx;
}

.tui-tool-item {
  width: 100%;
  display: flex;
  align-items: center;
  padding-top: 30rpx;
}

.tui-tool-item .tui-tool-right{
  flex: 1;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.tui-tool-item .tui-tool-right .icon-text{
  
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
}

.tui-tool-item .right-icon{
  width: 56rpx;
  height: 56rpx;
}

.tui-tool-icon {
  width: 30rpx;
  height: 30rpx;
  display: block;
}

.tui-badge-icon {
  width: 66rpx;
  height: 30rpx;
  position: absolute;
  right: 0;
  transform: translateX(88%);
  top: -15rpx;
}

/*为你推荐*/
.tui-product-list {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: wrap;
  box-sizing: border-box;
  margin-bottom: 100rpx;
}

.tui-product-container {
  flex: 1;
  margin-right: 2%;
}

.tui-product-container:last-child {
  margin-right: 0;
}

.tui-pro-item {
  width: 100%;
  margin-bottom: 4%;
  background: #fff;
  box-sizing: border-box;
  border-radius: 12rpx;
  overflow: hidden;
}

.tui-pro-img {
  width: 100%;
  display: block;
}

.tui-pro-content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 20rpx;
}

.tui-pro-tit {
  color: #2e2e2e;
  font-size: 26rpx;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.tui-pro-price {
  padding-top: 18rpx;
}

.tui-sale-price {
  font-size: 34rpx;
  font-weight: 500;
  color: #e41f19;
}

.tui-factory-price {
  font-size: 24rpx;
  color: #a0a0a0;
  text-decoration: line-through;
  padding-left: 12rpx;
}

.tui-pro-pay {
  padding-top: 10rpx;
  font-size: 24rpx;
  color: #656565;
}

.tui-pay-item__title {
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
}

.tui-pay-amuont {
  color: #eb0909;
  font-weight: 500;
  font-size: 34rpx;
}

.tui-pay-item {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
}

.tui-pay-logo {
  width: 48rpx;
  height: 48rpx;
  margin-right: 15rpx;
}

.tui-radio {
  margin-left: auto;
  transform: scale(0.8);
  transform-origin: 100% center;
}

.tui-btn-pay {
  width: 100%;
  padding: 50rpx;
  box-sizing: border-box;
}
